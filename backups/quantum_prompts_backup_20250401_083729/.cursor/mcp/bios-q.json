{
    "name": "bios-q",
    "version": "1.0.0",
    "description": "Quantum System Initialization Protocol",
    "type": "stdio",
    "autostart": true,
    "required": true,
    "config": {
        "paths": {
            "root": "/c/Eva Guarani EGOS",
            "quantum_prompts": "/c/Eva Guarani EGOS/QUANTUM_PROMPTS",
            "bios_backup": "/c/Eva Guarani EGOS/BIOS-Q_backup",
            "logs": "/c/Eva Guarani EGOS/logs"
        },
        "initialization": {
            "verify_structure": true,
            "load_context": true,
            "verify_dependencies": true,
            "display_banner": true
        },
        "logging": {
            "level": "DEBUG",
            "format": "quantum",
            "file": "bios_q.log"
        }
    },
    "env": {
        "DEBUG": "*",
        "NODE_ENV": "development",
        "NO_COLOR": "1",
        "FORCE_COLOR": "0",
        "QUANTUM_LOG_LEVEL": "DEBUG",
        "QUANTUM_STATE_DIR": "/c/Eva Guarani EGOS/QUANTUM_PROMPTS",
        "BIOS_Q_CONFIG": "/c/Eva Guarani EGOS/BIOS-Q_backup/config/bios_q_config.json"
    }
}
