{
  "system": {
    "name": "EVA & GUARANI",
    "version": "8.1",
    "language": "en",
    "debug_mode": false,
    "log_level": "INFO"
  },
  "paths": {
    "root": "./",
    "bios_q": "BIOS-Q/",
    "quantum_prompts": "QUANTUM_PROMPTS/",
    "subsystems": "subsystems/",
    "src": "src/",
    "tools": "tools/",
    "metadata": ".metadata/",
    "external": "external/",
    "logs": "logs/"
  },
  "database": {
    "type": "sqlite",
    "path": ".metadata/metadata_db.sqlite",
    "backup_path": "external/backups/"
  },
  "api": {
    "openai": {
      "api_key": "your-openai-api-key-here",
      "model": "gpt-4-turbo-preview",
      "temperature": 0.7,
      "max_tokens": 4000
    },
    "huggingface": {
      "api_key": "your-huggingface-api-key-here",
      "default_model": "microsoft/phi-2"
    }
  },
  "security": {
    "encryption_key": "your-encryption-key-here",
    "ssl_verify": true,
    "allowed_origins": ["http://localhost:3000"]
  },
  "features": {
    "auto_backup": true,
    "state_preservation": true,
    "quantum_analysis": true,
    "ethical_validation": true
  },
  "logging": {
    "file_handler": {
      "filename": "logs/eva_guarani.log",
      "max_bytes": 10485760,
      "backup_count": 5,
      "encoding": "utf-8"
    },
    "console_handler": {
      "level": "INFO",
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    }
  }
} 